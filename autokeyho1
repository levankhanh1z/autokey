local UserInputService = game:GetService("UserInputService")

local autoKeyPressing = false

function keypress(key)
    -- Thêm logic key press tại đây
    print("Key pressed:", key)
end

function keyrelease(key)
    -- Thêm logic key release tại đây
    print("Key released:", key)
end

UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
    if not gameProcessedEvent and input.KeyCode == Enum.KeyCode.X then
        autoKeyPressing = not autoKeyPressing
        if autoKeyPressing then
            autoKeyPress()
        end
    end
end)

function autoKeyPress()
    while autoKeyPressing do
        keypress(Enum.KeyCode.W) -- Thay đổi key code theo ý muốn
        wait(0.1) -- Đợi 0.1 giây (có thể thay đổi khoảng thời gian này)
        keyrelease(Enum.KeyCode.W)
        wait(0.1) -- Đợi 0.1 giây trước khi thực hiện lặp lại (có thể thay đổi khoảng thời gian này)
    end
end
